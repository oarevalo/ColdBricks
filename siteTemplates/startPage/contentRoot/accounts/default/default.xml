<?xml version="1.0" encoding="UTF-8"?>
<Page owner="default">
	<title>Start</title>
	<skin id="default" />
	<!-- Define Screen Layout -->
	<layout>
		<location class="column" name="left" type="column"/>
		<location class="column" name="middle" type="column"/>
		<location class="column" name="right" type="column"/>
	</layout>

	<!-- Define Modules -->
	<modules>
		<module id="FlickrFeed1" location="left" 
				name="flickrFeed/FlickrFeed" onClickGotoFlickr="true" 
				showheader="false" tags="colors" title="FlickrFeed1" maxItems="10" />
		<content id="welcome" location="middle" resourceID="default_welcome" title="About StartPage" />
		<module id="rssReader2" location="middle" name="RSSReader/rssReader"
				 rss="http://digg.com/rss/index.xml" title="Digg"
				displayMode="short"  maxItems="10" />	
		<module id="YouTube1" location="right" name="YouTube/youTubeVideo"
				videoID="Ic6O8H2t2iY" title="YouTube Videos!"
				width="250" height="250" />
	</modules>
	<eventListeners>
	</eventListeners>
	
</Page>
